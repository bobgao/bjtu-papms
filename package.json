{
  "private": true,
  "scripts": {
    "test": "umi test",
    "build": "umi build && scp dist/index.html imhele:/home/sites/imhele/bjtu-papms/",
    "precommit": "lint-staged",
    "lint": "yarn lint:es && yarn lint:ts",
    "lint:es": "eslint --ext .js src mock tests",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "start": "yarn run cleanCache && clear && umi dev",
    "start:win": "yarn run cleanCache:win && cls && umi dev",
    "pull:hg": "cd .. && hg pull && hg update",
    "push:hg": "cd .. && hg add . && hg commit -m \"update\" && hg push",
    "cleanCache": "rm -r src/pages/.umi && rm -r node_modules/.cache",
    "cleanCache:win": "del /Q /S src\\pages\\.umi && del /Q /S node_modules\\.cache"
  },
  "dependencies": {
    "antd": "^3.12.1",
    "classnames": "^2.2.6",
    "dva": "^2.5.0-beta.2",
    "memoize-one": "^5.0.0",
    "rc-queue-anim": "^1.6.10",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "^23.3.12",
    "@types/memoize-one": "^4.1.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.4.2",
    "umi-plugin-nprogress": "^1.1.6",
    "umi-plugin-oss": "^1.1.7",
    "umi-plugin-react": "^1.4.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
